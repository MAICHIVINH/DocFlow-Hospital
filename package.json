{
    "name": "docflow-backend",
    "version": "1.0.0",
    "description": "Hospital Document Transfer Management System Backend",
    "main": "src/server.js",
    "scripts": {
        "start": "node src/server.js",
        "dev": "nodemon src/server.js",
