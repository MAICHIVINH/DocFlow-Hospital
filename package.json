        "jsonwebtoken": "^9.0.2",
        "minio": "^8.0.6",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.7",
        "notistack": "^3.0.2",
        "pdfkit": "^0.17.2",
        "pg": "^8.11.3",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.35.1",
